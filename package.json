{
  "name": "4chan-X-Plus",
  "version": "3.7.1",
  "description": "Cross-browser extension for productive lurking on 4chan, a fork of 4chan X.",
  "meta": {
    "name": "4chan X Plus",
    "repo": "https://github.com/N3X15/4chan-x/",
    "page": "http://ci.nexisonline.net/job/4chan-x-plus/",
    "buildsPath": "builds/",
    "mainBranch": "v3",
    "matches": [
      "*://api.4chan.org/*",
      "*://boards.4chan.org/*",
      "*://images.4chan.org/*",
      "*://sys.4chan.org/*"
    ]
  },
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-bump": "~0.0.11",
    "grunt-concurrent": "~0.3.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-coffee": "~0.7.0",
    "grunt-contrib-compress": "~0.5.2",
    "grunt-contrib-concat": "~0.3.0",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-watch": "~0.5.0",
    "grunt-shell": "~0.3.1",
    "matchdep": "~0.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/N3X15/4chan-x.git"
  },
  "author": "Nicolas Stepien <stepien.nicolas@gmail.com>",
  "contributors": [
    "James Campos <james.r.campos@gmail.com>",
    "Rob Nelson <nexis@7chan.org>"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=0.10"
  }
